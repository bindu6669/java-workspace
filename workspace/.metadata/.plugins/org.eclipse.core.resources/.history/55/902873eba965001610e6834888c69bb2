
public class Bindu {
public static void main (String[] args){
	int b1 = 0B1111;
	int b2 = 0B0001;
	System.out.println(b1&b2);
	
	int c1 = 0XFACE;
	int c2 = 0X0001;
	System.out.println(c1&c2);
	
	int d1= 0007;
	int d2= 0001;
	System.out.println(d1&d2);
	



	int x=10;
	int y=15;
	System.out.println(x&y);
	System.out.println(x|y);
	
	
	if(++x<10 || ++y>15)	{
	   x++;
	}
	else
	{
	    y++;
	}

	System.out.println("value of x is " +x);
	System.out.println("value of y is " +y);
	
	
	if(++x<10 && ++y>15)  {
		x++;
	}
	else
	{
		y++;
	}
	
	System.out.println("value of x is" +x);
	System.out.println("value of y is" +y);
	
	int z= 8;
	if(++z<10 && z ==10 && z/0>10){
		System.out.println("hello");
	}
	else
	{
		System.out.println("in else loop");
		
		int a= 11 & 9;
		int b= a ^ 3;
		System.out.println( b | 12);
		
		boolean a11= true;
		boolean a22= false;
		boolean a33= true;
		if (a11 & a22 | a22 & a33 | a22)
		   System.out.println("Hello");
		if (a11 & a22 | a22 & a33 | a22| a11)
		    System.out.println("Guys");
		
		int e,f,g,h;
		e=f=g=h=20;
		e+=f-=g*=h/=2;
		System.out.println(e+ "__" + f + "__" + g + "__" + h);
		
		
		int i=10;
		i++;
		System.out.println(i);
		int j=10;
		++j;
		System.out.println(j);
		
		
		boolean b11 = true;
        boolean b22 = false;
        boolean b33 = true;
        if ( b11 & b22 | b22 & b33 | b22 ) 
            System.out.println("Hello");
        if ( b11 & b22 | b22 & b33 | b22 | b11 )
            System.out.println("Guys");
		
		
		int p=3;
		int q=4;
		System.out.println(" " + 7 + 2 + " ");
		System.out.println(p+q);
		System.out.println(" " + p+q + " ");
		
		int i1= 35;
		String s= "";
		s+=(i1%3==0) ? "coza": "";
		s+=(i1%5==0) ? "loza": "";
		s+=(i1%7==0)? "woza": "";
		
		System.out.println(s);
		
		
	 int sum =0;
	 int num=3;
	 sum+=num;
	 num+=2;
	 while (sum<= 100 );
	 {
     System.out.println("hello");	
     }
	
	}
	
	
	
	

	
}

