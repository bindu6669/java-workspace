
public class inverttriangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] num= new int [5][];
		
		
			  for (int j=0;j<=num.length-1;j++)
			  {
				  
				  System.out.println( "4" + " ");  
				  
			  }
			  System.out.println( "\n");
			 
			 }
			
	}


