
public class inverttriangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] num= new int [4][];
		
 	  for (int i=0;i<=num.length-1;i--)
 	  {
			  for (int j=0;j<=num.length-1;j--)
			  {
				  
				  System.out.print( num[i][j] + " ");  
				  
			  }
			  System.out.print( "\n");
			 ;
			 }
			
	}

}
