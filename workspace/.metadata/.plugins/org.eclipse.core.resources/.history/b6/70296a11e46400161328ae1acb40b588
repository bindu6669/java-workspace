
public class Bindu {
public static void main (String[] args){
	int b1 = 0B1111;
	int b2 = 0B0001;
	System.out.println(b1&b2);
	
	int c1 = 0XFACE;
	int c2 = 0X0001;
	System.out.println(c1&c2);
	
	int d1= 0007;
	int d2= 0001;
	System.out.println(d1&d2);

	int x=10;
	int y=15;
	System.out.println(x&y);
	System.out.println(x|y);
	
	
	if(++x<10 || ++y>15)	{
	   x++;
	}
	else
	{
	    y++;
	}

	System.out.println("value of x is " +x);
	System.out.println("value of y is " +y);
	
	
	if(++x<10 && ++y>15)  {
		x++;
	}
	else
	{
		y++;
	}
	
	System.out.println("value of x is" +x);
	System.out.println("value of y is" +y);
	
	int z= 8;
	if(++z<10 && z ==10 && z/0>10){
		System.out.println("hello");
	}
	else
	{
		System.out.println("in else loop");
		
		int a= 11 & 9;
		int b= a ^ 3;
		System.out.println( b | 12);
		
		boolean a1= true;
		boolean a2= false;
		boolean a3= true;
		if (a1 & a2 | a2 & a3 | a2);
		System.out.println("hello");
		if (a1 & a2 | a2 & a3 | a2| a1);
		System.out.println("Guys");
		
		int e,f,g,h;
		e=f=g=h=20;
		e+=f-=g*=h/=2;
		System.out.println(e+ "__" + f + "__" + g + "__" + h);
		
		
		int i=10;
		i++;
		System.out.println(i);
		int j=10;
		++j;
		System.out.println(j);
		
		
	
	}
	
	}
	
	
	
	

	
}

